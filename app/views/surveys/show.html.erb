<%= form_for @survey do |form| %>
  <%= form.label :source_code %>
  <%= form.text_area :source_code %>

  <%= form.submit "Submit" %>
<% end %>

<% if @compile_result %>
  <%= content_tag(:div, class: "survey-compilation-result #{@compile_result.success ? 'success' : 'failure'}") do %>
    <%= @compile_result.result %>
  <% end %>
<% end %>
